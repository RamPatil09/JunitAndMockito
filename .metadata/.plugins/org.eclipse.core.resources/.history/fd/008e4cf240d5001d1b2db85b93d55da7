package com.xworkz.practicespringboot.SairaTravels;

public class CustomerDto {

	private long id;
	private String name;
	private long age;
	private String sex;

	public CustomerDto(long id, String name, long age, String sex) {
		super();
		this.id = id;
		this.name = name;
		this.age = age;
		this.sex = sex;
	}

}
